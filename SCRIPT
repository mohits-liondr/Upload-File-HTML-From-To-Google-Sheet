function doGet() {
  return HtmlService.createTemplateFromFile('demo').evaluate()
    .addMetaTag('viewport', 'width=device-width, initial-scale=1')
    .setXFrameOptionsMode(HtmlService.XFrameOptionsMode.ALLOWALL);
}

var url = 'SHEET LINK';
var sh = 'Sheet1';
var folderId = 'FOLDER ID';

function processForm(formdata) {
  var superscript = SuperScript.initSuper(url, sh);
  var formObject = {};
  formdata.forEach(function(element) {
    formObject[element.getName()] = element.getValue();
  });

  var files = ['fare_bill_attachment', 'additional_expense_attachment', 'courier_bill_attachment'];
  files.forEach(function(id) {
    if (formObject[id]) {
      var file = JSON.parse(formObject[id]);
      var uploadedFile = superscript.uploadFile(folderId, file.data, file.name);
      formObject[id] = uploadedFile.getUrl();
    } else {
      formObject[id] = '';
    }
  });

  var ss = SpreadsheetApp.openByUrl(url);
  var ws = ss.getSheetByName(sh);
  ws.appendRow([
    new Date(),
    formObject.date,
    formObject.employee_code,
    formObject.name,
    formObject.designation,
    formObject.hq_name,
    formObject.working_status,
    formObject.worked_with ? formObject.worked_with.join(', ') : '',
    formObject.immediate_reporting_manager,
    formObject.reporting_manager_mail,
    formObject.route,
    formObject.locality,
    formObject.km,
    formObject.mode_of_travel,
    formObject.fare,
    formObject.fare_bill_attachment,
    formObject.additional_expense,
    formObject.additional_expense_attachment,
    formObject.courier,
    formObject.courier_bill_attachment,
    formObject.remarks
  ]);
}
